
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_AuthorHeadComponentPartial")

<body class="inner_page invoice_page">
    <div class="full_container">
        <div class="inner_container">
            <!-- Sidebar  -->
            @await Component.InvokeAsync("_AuthorSideBarComponentPartial")

            <!-- end sidebar -->
            <!-- right content -->
            <div id="content">
                <!-- topbar -->
                @await Component.InvokeAsync("_AuthorNavbarComponentPartial")
                <!-- end topbar -->

                @RenderBody()

            
            </div>
        </div>
    </div>

   
        <div class="footer">
            <p>©@DateTime.Now.Year Tüm Hakları Saklıdır.  Designed by html.design.</p>
        </div>
    

    <!-- jQuery -->
    <script src="~/pluto-1.0.0/js/jquery.min.js"></script>
    <script src="~/pluto-1.0.0/js/popper.min.js"></script>
    <script src="~/pluto-1.0.0/js/bootstrap.min.js"></script>
    <!-- wow animation -->
    <script src="~/pluto-1.0.0/js/animate.js"></script>
    <!-- select country -->
    <script src="~/pluto-1.0.0/js/bootstrap-select.js"></script>
    <!-- owl carousel -->
    <script src="~/pluto-1.0.0/js/owl.carousel.js"></script>
    <!-- chart js -->
    <script src="~/pluto-1.0.0/js/Chart.min.js"></script>
    <script src="~/pluto-1.0.0/js/Chart.bundle.min.js"></script>
    <script src="~/pluto-1.0.0/js/utils.js"></script>
    <script src="~/pluto-1.0.0/js/analyser.js"></script>
    <!-- nice scrollbar -->
    <script src="~/pluto-1.0.0/js/perfect-scrollbar.min.js"></script>
    <script>
        var ps = new PerfectScrollbar('#sidebar');
    </script>
    <!-- fancy box js -->
    <script src="~/pluto-1.0.0/js/jquery-3.3.1.min.js"></script>
    <script src="~/pluto-1.0.0/js/jquery.fancybox.min.js"></script>
    <!-- custom js -->
    <script src="~/pluto-1.0.0/js/custom.js"></script>
    <!-- calendar file css -->
    <script src="~/pluto-1.0.0/js/semantic.min.js"></script>
</body>
</html>
