@model List<TagCloud>

<div class="single-sidebar-widget tag_cloud_widget">
    <h4 class="single-sidebar-widget__title">Etiketler</h4>
    <ul class="list">
        @foreach (var item in Model)
        {
            <li>
                <a href="#">@item.Title</a>
            </li>
        }     
    </ul>
</div>